@font-face {
  font-family: 'Cooper';
  src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
  font-weight: 900;
  font-style: normal;
  font-display: swap;
}

.login-box{
  /* background-image: url(https://i.loli.net/2020/10/30/qR5Fj34nf8sXwyL.jpg);
  background-repeat: no-repeat;
  background-size: 100% 100%; */
  height: 100vh;
  overflow: hidden;
  position: relative;
}

.login-back-image{
  width: 100%;
  height: 100%;
  position: absolute;
}

@keyframes listener{
  from {
    opacity: 0;
  }
  to{
    opacity: 1;
  }
}
@keyframes circleBorder{
  0% {
    border-radius: 0;
    opacity: 0;
    bottom: -100vw;
  }
  50%{
    border-radius: 50% 50% 0 0;
    opacity: 1;
    bottom: -40vw;
  }
  100%{
    border-radius: 50% 50% 0 0;
    opacity: 1;
    bottom: 0;
  }
}
@keyframes circleUp{
  from{
    bottom: -40vw;
  }
  to{
    bottom: 0;
  }
}

.down-circle{
  position: absolute;
  width: 100vw;
  height: 100vw;
  bottom: 0;
  background-color: rgba(88, 42, 42, 0.45);
  /* border-radius: 50% 50% 0 0; */
  animation: circleBorder 2s;
  animation-fill-mode: forwards;
}
.down-txt{
  /* position: absolute;
  bottom: 0;
  left: 0;
  right: 0; */
  margin: 180rpx 0 0 0;
  text-align: center;
  font-size: 160rpx;
  font-family: 'Cooper';
  color: white;
  animation: listener 3s;
  animation-fill-mode: forwards;
}

.login-btn-box{
  width: 100%;
  display: flex;
  justify-content: center;
  margin: 120rpx 0 0 0;
}
.login-btn{
  width: 80%;
  color: rgb(138, 99, 86);
  font-size: 40rpx;
  font-weight: bold;
}